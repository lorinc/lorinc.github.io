{
  "Organizational_Maturity": {
    "description": "Defines an organization's readiness to successfully identify, adopt, and scale AI solutions.",
    "subdimensions": [
      {
        "name": "AI Maturity Stage",
        "description": "Overall stage of AI adoption within the organization.",
        "levels": [
          "Exploring — limited experimentation, no formal strategy.",
          "Piloting — small-scale prototypes or departmental initiatives.",
          "Scaling — AI integrated into multiple workflows, with governance.",
          "Operationalized — AI embedded in core systems, continuous improvement loop."
        ],
        "metrics": [
          "Number of live AI systems",
          "Presence of AI strategy or roadmap",
          "Cross-departmental AI alignment"
        ]
      },
      {
        "name": "Data Maturity",
        "description": "Degree of data readiness and reliability for AI deployment.",
        "levels": [
          "Siloed — fragmented datasets, minimal governance.",
          "Governed — unified sources, data catalog and access control.",
          "Automated — pipelines, lineage tracking, data quality monitoring."
        ],
        "metrics": [
          "Data integration coverage",
          "Data quality and freshness KPIs",
          "Existence of centralized governance policies"
        ]
      },
      {
        "name": "Process Automation Level",
        "description": "Extent to which organizational processes are automated or AI-assisted.",
        "levels": [
          "Manual — primarily human-driven, no automation.",
          "Semi-automated — rule-based tools assist repetitive work.",
          "AI-assisted — learning systems augment or make decisions autonomously."
        ],
        "metrics": [
          "Percentage of automated workflows",
          "Cycle-time reduction achieved",
          "AI adoption in daily operations"
        ]
      },
      {
        "name": "Technical Stack Sophistication",
        "description": "Breadth and modernity of the technology and infrastructure supporting AI.",
        "levels": [
          "Excel-only — limited or local tool usage.",
          "Hybrid — mix of local tools and basic cloud integration.",
          "Modern cloud stack — scalable infrastructure, CI/CD, and MLops practices."
        ],
        "metrics": [
          "Cloud vs on-premise workload ratio",
          "Existence of CI/CD pipelines",
          "Availability of AI/ML toolkits (Vertex, SageMaker, etc.)"
        ]
      }
    ]
  },
